    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Go App in Chrome",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src",
            "cwd": "${workspaceFolder}",
            "env": {},
            "args": [],
            "postDebugTask": "open-chrome"
        },
        {
            "name": "Debug WMU Scheduler",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src",
            "cwd": "${workspaceFolder}",
            "env": {},
            "args": [],
            "console": "integratedTerminal",
            "showLog": true
        },
        {
            "name": "Debug WMU Scheduler (Debug Env)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src",
            "cwd": "${workspaceFolder}",
            "env": {
                "ENV_FILE": ".env.debug"
            },
            "args": [],
            "console": "integratedTerminal",
            "showLog": true
        },
        {
            "name": "Debug WMU Scheduler (No Browser)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src",
            "cwd": "${workspaceFolder}",
            "env": {},
            "args": [],
            "console": "integratedTerminal",
            "showLog": true,
            "stopOnEntry": false
        }
    ]
}